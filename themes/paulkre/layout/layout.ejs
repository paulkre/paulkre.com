		<title><% if (page.title) { %><%= page.title %> | <% } %><%= config.title %></title>
		<title><%= `${page.title ? `${page.title} | ` : ""}${config.title}` %></title>

		<meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

		<link
			rel="stylesheet"
			type="text/css"
			href="<%= url_for("/assets/fonts/roboto-condensed-v16-latin/font-face.css") %>"
